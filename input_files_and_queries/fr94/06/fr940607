<!-- PJG /ITAG -->

<!-- PJG ITAG l=25 g=1 f=3 -->
Unit Type XX_Numbers of Units 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=25 g=1 f=1 -->
[position 133+1_second unit-level field]
<!-- PJG 0012 frnewline -->
