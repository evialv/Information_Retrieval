<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=52 g=1 f=1 -->
PART 70_[AMENDED]
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=4 -->
Paragraph 1.
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
 The authority citation for part 70 continues to read as follows: 
<!-- PJG /ITAG -->

<!-- PJG QTAG 04 -->
<!-- PJG /QTAG -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG ITAG l=21 g=1 f=1 -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=1 f=4 -->
Authority:
<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=1 f=1 -->
 5 U.S.C. 301 and 552; 26 U.S.C. 4181, 4182, 5146, 5203, 5207, 5275, 5367, 5415, 5504, 5555, 5684(a), 5741, 5761(b),
6020, 6021, 6064, 6102, 6155, 6159, 6201, 6203, 6204, 6301, 6303, 6311, 6313, 6314, 6321, 6323, 6325, 6326, 6331&hyph;6343,
