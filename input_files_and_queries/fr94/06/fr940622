<!-- PJG /ITAG -->

<!-- PJG ITAG l=80 g=1 f=1 -->
&sect;675.48
<!-- PJG /ITAG -->

<!-- PJG ITAG l=89 g=1 f=1 -->
Procedures and records. 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

