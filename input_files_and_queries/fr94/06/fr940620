<!-- PJG ITAG l=80 g=1 f=1 -->
252.227&hyph;7016 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=89 g=1 f=1 -->
Rights in bid or proposal data
<!-- PJG 0012 frnewline -->

