<USBUREAU>Federal Grain Inspection Service </USBUREAU>
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=52 g=1 f=1 -->

<CFRNO>7 CFR Part 801</CFRNO>
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=55 g=1 f=1 -->
FGIS to Change Protein Reference Method
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<AGENCY>
<!-- PJG ITAG l=10 g=1 f=2 -->
AGENCY: 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
Federal Grain Inspection Service, USDA.
<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
