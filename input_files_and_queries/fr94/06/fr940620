<!-- PJG ITAG l=55 g=1 f=1 -->
Hunger Strikes, Inmate 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<AGENCY>
<!-- PJG ITAG l=10 g=1 f=2 -->
AGENCY:
<!-- PJG /ITAG -->

