<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=7 f=2 -->
XXX
<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=1 f=1 -->
 (b)(2)(iii); or
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=7 f=2 -->
XXX
<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=1 f=1 -->
