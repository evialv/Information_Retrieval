<!-- PJG ITAG l=90 g=1 f=4 -->
Federal Register
<!-- PJG /ITAG -->

<!-- PJG ITAG l=90 g=1 f=1 -->
&blank;/&blank;Vol. 59, No. 117&blank;/&blank;Monday, 
<!-- PJG 0012 frnewline -->
June 20, 1994&blank;/&blank;Proposed Rules 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=52 g=1 f=1 -->

<USDEPT>DEPARTMENT OF TRANSPORTATION</USDEPT>

<!-- PJG 0012 frnewline -->

