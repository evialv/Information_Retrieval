<!-- PJG 007 0104 intable -->
X1
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=01 g=1 f=3 -->
Gouania vitifolia
<!-- PJG /ITAG -->

<!-- PJG ITAG l=01 g=1 f=1 -->
 
<!-- PJG 007 0104 intable -->
X 
<!-- PJG 007 0104 intable -->
