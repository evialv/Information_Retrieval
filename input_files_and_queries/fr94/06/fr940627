<!-- PJG 0012 frnewline -->

<!-- PJG 007 0152 intable -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=01 g=1 f=4 -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=01 g=1 f=3 -->
Cyanea grimesiana 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=01 g=1 f=1 -->
ssp. 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=01 g=1 f=3 -->
obatae
<!-- PJG /ITAG -->

<!-- PJG ITAG l=01 g=1 f=1 -->
 
<!-- PJG 007 0104 intable -->
