<!-- PJG /ITAG -->

<!-- PJG ITAG l=14 g=1 f=1 -->
1254.1(a)
<!-- PJG 0012 frnewline -->
1254.1(d)
<!-- PJG 0012 frnewline -->
1254.2(a)
<!-- PJG 0012 frnewline -->
1254.2(c)
<!-- PJG 0012 frnewline -->
1254.2(d)
<!-- PJG 0012 frnewline -->
1254.4(a) wherever it appears
<!-- PJG 0012 frnewline -->
