<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=1 f=1 -->
 21 U.S.C. 111, 114, 114a, 115&hyph;117, 120, 121, 134b, 134f; 
<!-- PJG 0012 frnewline -->
7 CFR 2.17, 2.51, and 371.2(d). 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=80 g=1 f=1 -->
&sect;77.1 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=89 g=1 f=1 -->
