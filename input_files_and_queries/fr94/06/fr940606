<!-- PJG /ITAG -->

<!-- PJG ITAG l=22 g=1 f=1 -->

<!-- PJG 0012 frnewline -->

<!-- PJG 007 0120 intable -->
1. Organic HAP emissions: &lt;160 grams/liter (1.3 lbs/gallon) (less water) as applied. (63.747(c)(1))
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
&blank;
<!-- PJG 0012 frnewline -->

<!-- PJG 007 0104 intable -->

<!-- PJG 007 0170 intable -->
l
