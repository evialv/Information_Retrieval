<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
Modified accredited state,
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
 paragraph (2) is amended by removing ``New York,''. 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=80 g=1 f=1 -->
&sect;77.1 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=89 g=1 f=1 -->
[Amended]
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
3. In &sect;77.1, in the definition for 
<!-- PJG /ITAG -->

