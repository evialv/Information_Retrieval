<!-- PJG ITAG l=80 g=1 f=1 -->
&sect;882.720 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=89 g=1 f=1 -->
PHA unit selection policy. 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=37 g=1 f=1 -->
