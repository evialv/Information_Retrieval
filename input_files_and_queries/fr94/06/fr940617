<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=1 f=1 -->
 Secs. 201, 301, 501, 502, 503, 505, 506, 507, 
<!-- PJG 0012 frnewline -->
701 of the Federal Food, Drug, and Cosmetic Act (21 U.S.C. 321, 
<!-- PJG 0012 frnewline -->
331, 351, 352, 353, 355, 356, 357, 371). 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=80 g=1 f=1 -->
