S. 
<!-- PJG 0012 frnewline -->
saprophyticus
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
), 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
