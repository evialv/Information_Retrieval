<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
Date:
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
 July 7&hyph;8, 1994.
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
Time:
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
 8:30 a.m. to 5 p.m.
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
Room:
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
 415.
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

