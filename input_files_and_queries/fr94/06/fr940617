<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
; 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
Enterobacter 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
species; 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
