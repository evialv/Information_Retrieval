<DOCTITLE>Richmond Power Enterprise, L.P.; Petition</DOCTITLE>
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=41 g=1 f=2 -->

<RINDOCK>[Docket No. RP94&hyph;286&hyph;000]</RINDOCK>
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

