<!-- PJG STAG 4703 -->

<!-- PJG ITAG l=94 g=1 f=2 -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=69 g=1 f=2 -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=50 g=1 f=2 -->


<USDEPT>DEPARTMENT OF AGRICULTURE</USDEPT>

<!-- PJG /ITAG -->

<!-- PJG ITAG l=18 g=1 f=2 -->

<USBUREAU>Forest Service</USBUREAU>
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

