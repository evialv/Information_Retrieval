<!-- PJG STAG 4700 -->

<TABLE>

<!-- PJG tableformat 11,L2,i1,s10,8,6,8,8,6,8,8,6,8,8 -->

<!-- PJG 0012 frnewline -->

<!-- PJG ITAG l=95 g=1 f=1 -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=95 g=1 f=4 -->
Table 
<!-- PJG /ITAG -->

