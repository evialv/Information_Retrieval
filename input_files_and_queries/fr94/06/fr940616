<!-- PJG FTAG 4701 -->

<!-- PJG STAG 4701 -->

<!-- PJG ITAG l=90 g=1 f=1 -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=90 g=1 f=4 -->
Federal Register
<!-- PJG /ITAG -->

<!-- PJG ITAG l=90 g=1 f=1 -->
&blank;/&blank;Vol. 59, No. 115&blank;/&blank;Thursday, June 16, 1994&blank;/&blank;Rules and Regulations
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=52 g=1 f=1 -->

<USDEPT>DEPARTMENT OF THE INTERIOR </USDEPT>

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=18 g=1 f=1 -->

<USBUREAU>Fish and Wildlife Service</USBUREAU>
<!-- PJG /ITAG -->

<!-- PJG ITAG l=52 g=1 f=1 -->
50 CFR Part 17 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=91 g=1 f=1 -->

<RINDOCK>RIN 101&hyph;8AC38 </RINDOCK>
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=52 g=1 f=1 -->
DEPARTMENT OF COMMERCE 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=18 g=1 f=1 -->

<USBUREAU>National Oceanic and Atmospheric Administration</USBUREAU>
