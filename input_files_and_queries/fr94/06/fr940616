<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=1 f=1 -->
1200&hyph;1500 (classified).
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=1 f=3 -->
Place: 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=1 f=1 -->
Pentagon, Washington, DC.
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

