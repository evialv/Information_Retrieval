5. Consolidated Edison Co. of New York, Inc. 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=71 g=1 f=1 -->
[Docket No. ER94&hyph;1271&hyph;000] 
<!-- PJG 0012 frnewline -->

