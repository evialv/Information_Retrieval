<!-- PJG 0012 frnewline -->

<!-- PJG ITAG l=20 g=1 f=1 -->
``Firm Price $
<!-- PJG /ITAG -->

<!-- PJG ITAG l=20 g=7 f=2 -->
XXXX
<!-- PJG /ITAG -->

<!-- PJG ITAG l=20 g=1 f=1 -->

<!-- PJG 0012 frnewline -->
Nonfirm Price $
<!-- PJG /ITAG -->

<!-- PJG ITAG l=20 g=7 f=2 -->
XXXX
<!-- PJG /ITAG -->

<!-- PJG ITAG l=20 g=1 f=1 -->
''
