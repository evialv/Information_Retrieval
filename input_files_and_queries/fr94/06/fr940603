ss min
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
) 
<!-- PJG 007 0150 intable -->
2(C
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=5 f=2 -->
ss
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
) 
<!-- PJG 007 0150 intable -->
