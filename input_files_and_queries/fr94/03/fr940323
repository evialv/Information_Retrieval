Authority:
<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=1 f=1 -->
 42 U.S.C. 12501 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=1 f=3 -->
et seq. 
<!-- PJG 0012 frnewline -->

