<!-- PJG ITAG l=90 g=1 f=1 -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=90 g=1 f=4 -->
Federal Register
<!-- PJG /ITAG -->

<!-- PJG ITAG l=90 g=1 f=1 -->
&blank;/&blank;Vol. 59, No. 56&blank;/&blank;Wednesday, March 23, 1994&blank;/&blank;Proposed Rules
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=52 g=1 f=1 -->

<USDEPT>DEPARTMENT OF HEALTH AND HUMAN SERVICES</USDEPT>

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

