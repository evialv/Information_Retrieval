<!-- PJG ITAG l=52 g=1 f=1 -->
Pesticide Tolerances for Acetochlor
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
<!-- PJG /QTAG -->

<!-- PJG 0012 frnewline -->

<AGENCY>
<!-- PJG ITAG l=10 g=1 f=2 -->
