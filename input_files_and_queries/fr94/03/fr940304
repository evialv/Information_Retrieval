<!-- PJG 007 0104 intable -->
5,000 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
Petrochemicals exported 
<!-- PJG 007 0104 intable -->
10,000 
<!-- PJG 007 0162 intable -->
n,s
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=04 g=1 f=1 -->
Total removals 
<!-- PJG 007 0104 intable -->
70,000 
<!-- PJG /ITAG -->

</TABLE>

