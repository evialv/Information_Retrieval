<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=74 g=1 f=1 -->
Notice of Serious Interest 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=71 g=1 f=1 -->
Re: [insert name of property] 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=71 g=1 f=4 -->
Federal Register 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=71 g=1 f=1 -->
Publication Date: 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=71 g=7 f=2 -->
XXXXX
<!-- PJG /ITAG -->

<!-- PJG ITAG l=71 g=1 f=1 -->
 [insert 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=71 g=1 f=4 -->
Federal Register 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=71 g=1 f=1 -->
publication date] 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=1 f=1 -->
