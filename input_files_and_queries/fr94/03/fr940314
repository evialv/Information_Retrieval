Cl
<!-- PJG /ITAG -->

<!-- PJG ITAG l=16 g=5 f=2 -->
2
<!-- PJG /ITAG -->

<!-- PJG ITAG l=16 g=1 f=1 -->
 (chlorine) + C
<!-- PJG /ITAG -->

<!-- PJG ITAG l=16 g=5 f=2 -->
6
<!-- PJG /ITAG -->

<!-- PJG ITAG l=16 g=1 f=1 -->
H
<!-- PJG /ITAG -->

<!-- PJG ITAG l=16 g=5 f=2 -->
6
<!-- PJG /ITAG -->

<!-- PJG ITAG l=16 g=1 f=1 -->
 (benzene) + HNO
<!-- PJG /ITAG -->

<!-- PJG ITAG l=16 g=5 f=2 -->
3
<!-- PJG /ITAG -->

<!-- PJG ITAG l=16 g=1 f=1 -->
 (nitric acid) __> 
<!-- PJG 0012 frnewline -->
C
<!-- PJG /ITAG -->

<!-- PJG ITAG l=16 g=5 f=2 -->
