<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
] 
<!-- PJG 0012 frnewline -->
First-Class residual pieces may be sequenced by ZIP Code and presented with a listing: 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

