<!-- PJG 0012 frnewline -->

<!-- PJG 007 0150 intable -->
1Margin 
<!-- PJG 007 0120 intable -->
(percent)
<!-- PJG 0012 frnewline -->

<!-- PJG 007 0152 intable -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=01 g=1 f=1 -->
Companhia Ferroligas Minas Gerais 
<!-- PJG 007 0104 intable -->
3.46
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
Italmagnesio S.A. Industria e Comercio 
<!-- PJG 007 0104 intable -->
88.86 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
