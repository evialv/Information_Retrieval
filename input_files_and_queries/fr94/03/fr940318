<!-- PJG ITAG l=55 g=1 f=1 -->

<DOCTITLE>Nuveen EquityBuilder Municipal Unit Investment Trust; Notice of Application</DOCTITLE>
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=44 g=1 f=1 -->
March 11, 1994.
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<AGENCY>
<!-- PJG ITAG l=10 g=1 f=2 -->
AGENCY:
