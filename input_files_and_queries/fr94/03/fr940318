<!-- PJG tableformat 5,L2,i1,s50,r50,xs56,r100,r100 -->

<!-- PJG 0012 frnewline -->

<!-- PJG ITAG l=95 g=1 f=4 -->
Fire Suppression and Explosion Protection Total Flooding Agents
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=96 g=1 f=4 -->
Substitutes Acceptable Subject To Use Conditions
<!-- PJG 0012 frnewline -->

<!-- PJG 007 0150 intable -->
1End-use
<!-- PJG 0012 frnewline -->

