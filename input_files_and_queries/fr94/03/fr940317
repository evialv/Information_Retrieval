<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
 July 1, 1993, through June 30, 1994.
<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
<!-- PJG /QTAG -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
</DATE>
<FURTHER>
<!-- PJG ITAG l=10 g=1 f=2 -->
