<!-- PJG 0012 frnewline -->
taxpayer, this section is effective for taxable years beginning 
<!-- PJG 0012 frnewline -->
after December 31, 1986. 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=80 g=1 f=1 -->
&sect;1.263A&hyph;1T
<!-- PJG /ITAG -->

<!-- PJG ITAG l=89 g=1 f=1 -->
