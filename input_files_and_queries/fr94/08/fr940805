<!-- PJG ITAG l=80 g=1 f=1 -->
&sect;1.263A&hyph;4T
<!-- PJG /ITAG -->

<!-- PJG ITAG l=89 g=1 f=1 -->
 Rules relating to property produced in a farming 
<!-- PJG 0012 frnewline -->
business. 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

