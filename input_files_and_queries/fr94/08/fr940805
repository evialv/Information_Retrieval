<!-- PJG 0012 frnewline -->
Severely Disabled.
<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
<!-- PJG /QTAG -->
 
<!-- PJG 0012 frnewline -->
</AGENCY>
<ACTION>
<!-- PJG ITAG l=10 g=1 f=2 -->
ACTION:
<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
 Addition to the Procurement List.
<!-- PJG /ITAG -->

<!-- PJG ITAG l=59 g=1 f=1 -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->
</ACTION>
<SUMMARY>
<!-- PJG ITAG l=10 g=1 f=2 -->
SUMMARY:
<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
 This action adds to the Procurement List a commodity to 
<!-- PJG 0012 frnewline -->
be furnished by nonprofit agencies employing persons who are blind 
<!-- PJG 0012 frnewline -->
or have other severe disabilities. 
<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
<!-- PJG /QTAG -->

<!-- PJG 0012 frnewline -->
</SUMMARY>
<DATE>
<!-- PJG ITAG l=10 g=1 f=2 -->
