<USDEPT>FEDERAL HOUSING FINANCE BOARD</USDEPT>

<!-- PJG /ITAG -->

<!-- PJG ITAG l=41 g=1 f=1 -->

<RINDOCK>[No. 94&hyph;NO3] </RINDOCK>
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=55 g=1 f=1 -->
Prices for Federal Home Loan Bank Services 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<AGENCY>
<!-- PJG ITAG l=10 g=1 f=2 -->
AGENCY:
<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
 Federal Housing Finance Board.
<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
