<!-- PJG 0020 minuteslatlong -->
&rsquo;00
<!-- PJG 0020 minuteslatlong -->
&rsquo;
<!-- PJG 0020 minuteslatlong -->
&rsquo;N., 168
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=8 f=1 -->

<!-- PJG 0253 grid5678 -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
00
<!-- PJG 0020 minuteslatlong -->
&rsquo;00
<!-- PJG 0020 minuteslatlong -->
&rsquo;
<!-- PJG 0020 minuteslatlong -->
&rsquo;W. to Cape Newenham (58
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=8 f=1 -->

<!-- PJG 0253 grid5678 -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
39
<!-- PJG 0020 minuteslatlong -->
&rsquo;00
<!-- PJG 0020 minuteslatlong -->
&rsquo;
<!-- PJG 0020 minuteslatlong -->
&rsquo;N., 162
<!-- PJG /ITAG -->

