<FOOTNAME>
<!-- PJG ITAG l=28 g=1 f=1 -->
15
<!-- PJG /ITAG -->

</FOOTNAME>
<!-- PJG ITAG l=28 g=1 f=1 -->
17 CFR 200.30&hyph;3(a)(12).
<!-- PJG 0012 frnewline -->

