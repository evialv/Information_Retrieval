<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
1545&hyph;0118.
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
Form Number: 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
IRS Form 1099&hyph;PATR.
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

