33 CFR Part 168
<!-- PJG 0012 frnewline -->
Escort Vessels for Certain Tankers; Final Rule
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG /STAG -->

<!-- PJG /FTAG -->

<!-- PJG FTAG 4701 -->

<!-- PJG STAG 4701 -->

<!-- PJG ITAG l=90 g=1 f=4 -->
Federal Register
<!-- PJG /ITAG -->

<!-- PJG ITAG l=90 g=1 f=1 -->
&blank;/&blank;Vol. 59, No. 160&blank;/&blank;Friday, 
<!-- PJG 0012 frnewline -->
August 19, 1994&blank;/&blank;Rules and Regulations 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=52 g=1 f=1 -->

