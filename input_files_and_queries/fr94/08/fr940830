<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG ITAG l=10 g=1 f=1 -->
where the BCF
<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=5 f=4 -->
l
<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=5 f=1 -->
