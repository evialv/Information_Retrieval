<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=52 g=1 f=1 -->
PART 319_FOREIGN QUARANTINE NOTICES 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

