<!-- PJG ITAG l=55 g=1 f=1 -->
Petitions for Exemption; Summary of Petitions Received; Dispositions of Petitions Issued
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<AGENCY>
<!-- PJG ITAG l=10 g=1 f=2 -->
