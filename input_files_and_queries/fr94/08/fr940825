25 CFR Part 46
<!-- PJG 0012 frnewline -->
Adult Education Program; Proposed Rule
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG /STAG -->

<!-- PJG /FTAG -->

<!-- PJG FTAG 4701 -->

<!-- PJG STAG 4701 -->

<!-- PJG ITAG l=90 g=1 f=1 -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=90 g=1 f=4 -->
Federal Register
<!-- PJG /ITAG -->

<!-- PJG ITAG l=90 g=1 f=1 -->
&blank;/&blank;Vol. 59, No. 164&blank;/&blank;Thursday, August 25, 1994&blank;/&blank;Proposed Rules
<!-- PJG 0012 frnewline -->
 
<!-- PJG /ITAG -->

<!-- PJG /STAG -->

<!-- PJG /FTAG -->

<!-- PJG STAG 4702 -->

<!-- PJG ITAG l=52 g=1 f=1 -->
DEPARTMENT OF THE INTERIOR 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=18 g=1 f=1 -->

