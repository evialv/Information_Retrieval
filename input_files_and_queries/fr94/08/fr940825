<!-- PJG ITAG l=01 g=1 f=1 -->
Sandvik 
<!-- PJG 007 0104 intable -->
05/22/87&hyph;11/30/88 
<!-- PJG 007 0104 intable -->
3.65
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
Sandvik 
<!-- PJG 007 0104 intable -->
