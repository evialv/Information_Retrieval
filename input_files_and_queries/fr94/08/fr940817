<USDEPT>DEPARTMENT OF THE TREASURY</USDEPT>

<!-- PJG /ITAG -->

<!-- PJG ITAG l=18 g=1 f=1 -->

<USBUREAU>Bureau of Alcohol, Tobacco and Firearms</USBUREAU>
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=52 g=1 f=1 -->

<CFRNO>27 CFR Parts 4, 5, and 7 </CFRNO>
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=41 g=1 f=1 -->

<RINDOCK>[T.D. ATF&hyph;359] </RINDOCK>
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=91 g=1 f=1 -->

<RINDOCK>RIN 1512&hyph;AB31 </RINDOCK>
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=55 g=1 f=1 -->
Signature Authority for Approval of Certificates of Label Approval (COLAS) (94F&hyph;002P) 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<AGENCY>
<!-- PJG ITAG l=10 g=1 f=2 -->
AGENCY:
<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
 Bureau of Alcohol, Tobacco and Firearms (ATF), Treasury.
<!-- PJG /ITAG -->

