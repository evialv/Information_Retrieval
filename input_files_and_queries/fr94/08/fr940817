10
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
 
<!-- PJG 0012 frnewline -->
value of 87 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=6 f=1 -->
m
<!-- PJG /ITAG -->

