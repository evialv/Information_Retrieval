<!-- PJG /ITAG -->

<!-- PJG ITAG l=80 g=1 f=1 -->
&sect;30.36 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=89 g=1 f=1 -->
Expiration and termination of licenses. 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

