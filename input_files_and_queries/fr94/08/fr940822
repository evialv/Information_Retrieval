<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=52 g=1 f=1 -->
PART 135_RULES FOR MEASUREMENT OF VESSELS 
<!-- PJG 0012 frnewline -->

