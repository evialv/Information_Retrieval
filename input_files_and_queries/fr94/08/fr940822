<!-- PJG ITAG l=55 g=1 f=1 -->
Airworthiness Directives; Jetstream Model 4101 Airplanes 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<AGENCY>
<!-- PJG ITAG l=10 g=1 f=2 -->
