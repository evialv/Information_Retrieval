<!-- PJG ITAG l=95 g=1 f=1 -->
Table of Contents
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG 007 0150 intable -->
1&blank;
<!-- PJG 007 0150 intable -->
1Paragraph
<!-- PJG 007 0152 intable -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=01 g=1 f=1 -->
I. Introduction 
<!-- PJG 007 0104 intable -->
1
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
II. Applicability of Competitive Bidding 
