<!-- PJG ITAG l=55 g=1 f=1 -->
Medicare Program; Meeting of the Practicing Physicians 
<!-- PJG 0012 frnewline -->
Advisory Council 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<AGENCY>
<!-- PJG ITAG l=10 g=1 f=2 -->
