<!-- PJG /ITAG -->

<!-- PJG ITAG l=28 g=1 f=3 -->
See 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=28 g=1 f=1 -->
OEX RAES Approval Order, 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=28 g=1 f=3 -->
supra 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=28 g=1 f=1 -->
note 5.
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->
</FOOTNOTE>

<FOOTNOTE>
<FOOTNAME>
<!-- PJG ITAG l=28 g=1 f=1 -->
11
<!-- PJG /ITAG -->

</FOOTNAME>
<!-- PJG ITAG l=28 g=1 f=1 -->

<!-- PJG /ITAG -->

