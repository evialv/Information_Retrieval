<!-- PJG /ITAG -->

<!-- PJG ITAG l=12 g=1 f=3 -->
Streptococcus pneumoniae 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=12 g=1 f=1 -->
disease, including meningitis, pneumonia, sinusitis, and otitis media 
<!-- PJG 0012 frnewline -->

