<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
 Request for comments on U.S. international air transportation policy statement. 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=59 g=1 f=1 -->

