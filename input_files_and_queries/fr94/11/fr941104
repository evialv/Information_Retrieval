<!-- PJG ITAG l=55 g=1 f=1 -->
Distribution of Fiscal Year 1995 Contract Support Funds 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=44 g=1 f=1 -->
October 28, 1994. 
<!-- PJG 0012 frnewline -->
 
<!-- PJG 0012 frnewline -->
 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<AGENCY>
<!-- PJG ITAG l=10 g=1 f=2 -->
AGENCY: 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
Bureau of Indian Affairs, Interior. 
<!-- PJG 0012 frnewline -->
 
<!-- PJG /ITAG -->

<!-- PJG QTAG 03 -->
<!-- PJG /QTAG -->

<!-- PJG 0012 frnewline -->
 
<!-- PJG 0012 frnewline -->
 
<!-- PJG 0012 frnewline -->
