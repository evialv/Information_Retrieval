<!-- PJG STAG 4700 -->

<!-- PJG ITAG l=11 g=1 f=1 -->

<!-- PJG 0012 frnewline -->
(2) A determination regarding services that are not covered under the HCPP's agreement with HCFA. 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=80 g=1 f=1 -->

<!-- PJG 0012 frnewline -->
&sect;417.840 
<!-- PJG /ITAG -->

