<!-- PJG ITAG l=84 g=1 f=1 -->
4. Columbia Gas Transmission Corporation 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=71 g=1 f=1 -->
[Docket No. CP95&hyph;34&hyph;000] 
