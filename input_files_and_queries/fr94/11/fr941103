<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
 This policy statement is effective as of 
<!-- PJG 0012 frnewline -->
October 26, 1994. 
<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
<!-- PJG /QTAG -->

<!-- PJG 0012 frnewline -->
</DATE>
