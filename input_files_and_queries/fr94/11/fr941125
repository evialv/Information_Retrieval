<!-- PJG /ITAG -->

<!-- PJG ITAG l=16 g=5 f=2 -->
6
<!-- PJG /ITAG -->

<!-- PJG ITAG l=16 g=1 f=1 -->
 (butadiene) 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=16 g=7 f=3 -->
c
<!-- PJG /ITAG -->

<!-- PJG ITAG l=16 g=1 f=1 -->
 C
<!-- PJG /ITAG -->

<!-- PJG ITAG l=16 g=5 f=2 -->
