<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
Comments must be received on or before January 23, 1995.
<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
<!-- PJG /QTAG -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
</DATE>
