<USDEPT>DEPARTMENT OF AGRICULTURE</USDEPT>

<!-- PJG /ITAG -->

<!-- PJG ITAG l=18 g=1 f=1 -->

<USBUREAU>Farmers Home Administration</USBUREAU>
<!-- PJG 0012 frnewline -->

