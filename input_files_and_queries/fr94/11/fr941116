<!-- PJG ITAG l=55 g=1 f=1 -->

<DOCTITLE>Determination of Regulatory Review Period for Purposes of Patent Extension; Zerit
<!-- PJG /ITAG -->

<!-- PJG ITAG l=55 g=6 f=2 -->
&reg;
<!-- PJG /ITAG -->

<!-- PJG ITAG l=55 g=1 f=1 -->
</DOCTITLE>
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<AGENCY>
<!-- PJG ITAG l=10 g=1 f=2 -->
