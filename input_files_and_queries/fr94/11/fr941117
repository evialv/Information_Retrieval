<!-- PJG 0012 frnewline -->

<!-- PJG 007 0152 intable -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=01 g=1 f=4 -->
35 to 512 MHz 
<!-- PJG 007 0104 intable -->
80
<!-- PJG 0017 degreeslatlong -->
