<!-- PJG ITAG l=28 g=1 f=3 -->
See
<!-- PJG /ITAG -->

<!-- PJG ITAG l=28 g=1 f=1 -->
 Rule 15c2&hyph;12(a). 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->
</FOOTNOTE>

