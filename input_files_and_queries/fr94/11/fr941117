<!-- PJG /ITAG -->

<!-- PJG ITAG l=15 g=1 f=1 -->
 3,600
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=15 g=1 f=3 -->
Responses per Respondent:
<!-- PJG /ITAG -->

<!-- PJG ITAG l=15 g=1 f=1 -->
 1
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=15 g=1 f=3 -->
