<DOCNO> FR941117-0-00130 </DOCNO>
<PARENT> FR941117-0-00011 </PARENT>
<TEXT>
 
<!-- PJG STAG 4700 -->

<TABLE>

<!-- PJG tableformat 2,L2,tp0,i1,s25,r100 -->

