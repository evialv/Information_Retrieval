<!-- PJG 0012 frnewline -->

<!-- PJG 007 0104 intable -->

<!-- PJG 007 0170 intable -->
l6,105,310,400 (16.7%)
<!-- PJG 0012 frnewline -->

<!-- PJG 007 0104 intable -->

<!-- PJG 007 0170 intable -->
l2,505,600 (0.01%) 
<!-- PJG 0012 frnewline -->

<!-- PJG 007 0146 intable -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=01 g=5 f=1 -->
