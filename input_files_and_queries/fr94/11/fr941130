<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
50 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
Number of Responses per Respondent: 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
