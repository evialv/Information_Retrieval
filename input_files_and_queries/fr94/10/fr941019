<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=02 g=1 f=4 -->
(Local and Out of Town Accounts) 
<!-- PJG 007 0104 intable -->
$170.00
<!-- PJG 0012 frnewline -->
(Specialist, Trading and Market Maker Accounts) 
<!-- PJG 007 0104 intable -->
160.00
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

