2. Ram Head Coal Company 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=71 g=1 f=1 -->
[Docket No. M&hyph;94&hyph;140&hyph;C] 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

