<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
 installation of a key microswitch and a warning buzzer in the steering lock assembly.
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->
</SUPPLEM>
