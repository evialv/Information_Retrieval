<!-- PJG 0012 frnewline -->

<!-- PJG 007 0152 intable -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=22 g=1 f=1 -->
&blank;
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=28 g=1 f=1 -->
*&blank;&blank;&blank;&blank;*&blank;&blank;&blank;&blank;*&blank;&blank;&blank;&blank;*&blank;&blank;&blank;&blank;*
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=01 g=1 f=1 -->
Pome fruit
<!-- PJG 0012 frnewline -->

<!-- PJG 007 0104 intable -->
10
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=22 g=1 f=1 -->
&blank;
<!-- PJG 0012 frnewline -->

