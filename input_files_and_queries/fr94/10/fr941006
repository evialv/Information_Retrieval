<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=24 g=1 f=1 -->
Sec. 33, SW1/4SE1/4.
<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
