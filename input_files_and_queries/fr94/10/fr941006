<!-- PJG 0012 frnewline -->
The revisions read as follows: 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=80 g=1 f=1 -->
&sect;246.16 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=89 g=1 f=1 -->
Distribution of funds. 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

