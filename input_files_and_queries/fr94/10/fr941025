<!-- PJG ITAG l=41 g=1 f=1 -->

<RINDOCK>[Regulation T; Docket No. 0840] </RINDOCK>
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=55 g=1 f=1 -->
Credit by Brokers and Dealers 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<AGENCY>
<!-- PJG ITAG l=10 g=1 f=2 -->
AGENCY:
<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
 Board of Governors of the Federal Reserve System. 
<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
