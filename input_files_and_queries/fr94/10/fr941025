48.3 (19) 
<!-- PJG 0012 frnewline -->

<!-- PJG 007 0104 intable -->
15.5 (61/8) 
<!-- PJG 0012 frnewline -->

<!-- PJG 007 0104 intable -->
21.7 (81/2)
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=1 f=2 -->
Size 14 Girls
<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
<!-- PJG /QTAG -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG ITAG l=22 g=1 f=2 -->
Maximum Dimension1 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=03 g=1 f=2 -->
Centimeters (inches) 
<!-- PJG 0012 frnewline -->

<!-- PJG 007 0104 intable -->
