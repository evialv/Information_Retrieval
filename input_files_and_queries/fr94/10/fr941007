<!-- PJG importformat s,d600 -->

<!-- PJG 0012 frnewline -->

<!-- PJG ITAG l=95 g=1 f=1 -->
E:GRAPHICSEP07OC94.005
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

</IMPORT>

<!-- PJG 0101 clearpage -->

