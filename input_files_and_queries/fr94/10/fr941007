<!-- PJG STAG 4700 -->

<!-- PJG ITAG l=85 g=1 f=1 -->
Table 151.26(b)(8)_Index of Sections_Sample Format
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=83 g=1 f=1 -->
Mandatory
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=15 g=1 f=1 -->
Section 1: Introduction
