<!-- PJG STAG 4702 -->

<!-- PJG ITAG l=94 g=1 f=1 -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=69 g=1 f=1 -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=52 g=1 f=1 -->
DEPARTMENT OF DEFENSE
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
GENERAL SERVICES ADMINISTRATION
<!-- PJG 0012 frnewline -->
NATIONAL AERONAUTICS AND SPACE ADMINISTRATION
<!-- PJG 0012 frnewline -->

<CFRNO>48 CFR Parts 22, 31, and 42</CFRNO>
<!-- PJG 0012 frnewline -->

