<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
 Internal Revenue Service (IRS), Treasury. 
<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
<!-- PJG /QTAG -->

<!-- PJG 0012 frnewline -->
</AGENCY>
<ACTION>
<!-- PJG ITAG l=10 g=1 f=2 -->
