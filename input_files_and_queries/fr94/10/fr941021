<!-- PJG 0012 frnewline -->

<!-- PJG 007 0150 intable -->
1Margin (percent)
<!-- PJG 007 0152 intable -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=01 g=1 f=1 -->
TOSOH
<!-- PJG 0012 frnewline -->

<!-- PJG 007 0104 intable -->
4/1/91&hyph;3/31/92
<!-- PJG 0012 frnewline -->

