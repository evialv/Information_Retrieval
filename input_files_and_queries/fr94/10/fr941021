<!-- PJG /ITAG -->

<!-- PJG ITAG l=28 g=1 f=1 -->
 at paras. 80&hyph;81.
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->
</FOOTNOTE>

<FOOTNOTE>
<FOOTNAME>
<!-- PJG ITAG l=28 g=1 f=1 -->
