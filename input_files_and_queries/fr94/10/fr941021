<!-- PJG /ITAG -->

</FOOTNAME>
<!-- PJG ITAG l=28 g=1 f=1 -->
Notice, note 2, 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=28 g=1 f=3 -->
supra,
<!-- PJG /ITAG -->

<!-- PJG ITAG l=28 g=1 f=1 -->
 at para. 80.
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->
</FOOTNOTE>

<FOOTNOTE>
<FOOTNAME>
<!-- PJG ITAG l=28 g=1 f=1 -->
