<!-- PJG /ITAG -->

<!-- PJG ITAG l=55 g=1 f=1 -->

<DOCTITLE>Performance Review Board </DOCTITLE>
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<AGENCY>
<!-- PJG ITAG l=10 g=1 f=2 -->
AGENCY:
<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
 Internal Revenue Service, Treasury.
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
<!-- PJG /QTAG -->

<!-- PJG 0012 frnewline -->
