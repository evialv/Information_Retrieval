<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
Other selection factors.
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
 * * * 
<!-- PJG 0012 frnewline -->

