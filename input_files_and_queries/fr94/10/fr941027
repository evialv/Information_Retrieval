<USDEPT>SECURITIES AND EXCHANGE COMMISSION</USDEPT>

<!-- PJG /ITAG -->

<!-- PJG ITAG l=52 g=1 f=1 -->

<CFRNO>17 CFR PART 200</CFRNO>
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=41 g=1 f=1 -->

<RINDOCK>[Release No. 34&hyph;34871]</RINDOCK>
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=55 g=1 f=1 -->
Disposition of Business by Seriatim Commission Consideration or by Delegated Authority
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<ACTION>
<!-- PJG ITAG l=10 g=1 f=2 -->
ACTION: 
