Executive Order 12866. 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=84 g=1 f=1 -->
Executive Order 12778 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

