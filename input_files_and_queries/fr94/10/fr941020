<!-- PJG /ITAG -->

<!-- PJG ITAG l=55 g=1 f=3 -->
Revocation of Public Land Order No. 5023; Arizona
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<AGENCY>
<!-- PJG ITAG l=10 g=1 f=2 -->
