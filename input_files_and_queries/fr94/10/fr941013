<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=1 f=3 -->
et
<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=1 f=1 -->
 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=1 f=3 -->
seq
<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=1 f=1 -->
.
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

