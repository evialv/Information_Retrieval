<!-- PJG FTAG 4717 -->

<!-- PJG STAG 4717 -->

<!-- PJG ITAG l=01 g=1 f=1 -->
Tuesday
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=02 g=1 f=1 -->
December 27, 1994
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=09 g=1 f=1 -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=03 g=1 f=1 -->
Part IV
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=04 g=1 f=1 -->
Department of Education
