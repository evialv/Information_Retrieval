<!-- PJG /ITAG -->

<!-- PJG ITAG l=41 g=1 f=1 -->
3] </RINDOCK>
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=55 g=1 f=1 -->
Texas; Declaration of Disaster Loan Area
<!-- PJG 0012 frnewline -->
 
<!-- PJG /ITAG -->

