follows:] 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=84 g=1 f=1 -->
2.3&blank;Heavy Letter Mail 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

