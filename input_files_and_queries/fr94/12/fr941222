<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
2. Section 52.2020 is amended by adding paragraph (c)(94) to read as follows:
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=80 g=1 f=1 -->
&sect;52.2020
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

