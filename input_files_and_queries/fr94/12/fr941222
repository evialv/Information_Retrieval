follows: 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=85 g=1 f=1 -->
Appendix A to Part 208_Capital Adequacy Guidelines for State Member Banks: Risk-Based Measure
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=37 g=1 f=1 -->
