<!-- PJG ITAG l=83 g=1 f=1 -->
620&blank;International Package Consignment Service 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
621&blank;Description 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

