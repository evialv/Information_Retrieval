<!-- PJG ITAG l=52 g=1 f=1 -->
PART 208_MEMBERSHIP OF STATE BANKING INSTITUTIONS IN THE FEDERAL RESERVE SYSTEM (REGULATION H) 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=1 f=1 -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=1 f=4 -->
1. 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=1 f=1 -->
