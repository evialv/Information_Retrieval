<!-- PJG ITAG l=52 g=1 f=1 -->
DEPARTMENT OF HOUSING AND URBAN DEVELOPMENT 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=18 g=1 f=1 -->

