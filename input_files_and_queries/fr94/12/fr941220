<DOCTITLE>Announcment of Meeting of National Conference on Weights and Measures</DOCTITLE>
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<AGENCY>
<!-- PJG ITAG l=10 g=1 f=2 -->
AGENCY: 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
National Institute of Standards and Technology, Commerce.
<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
<!-- PJG /QTAG -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
