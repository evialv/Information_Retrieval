<!-- PJG /FTAG -->

<!-- PJG STAG 4702 -->

<!-- PJG ITAG l=52 g=1 f=1 -->
DEPARTMENT OF HOUSING AND URBAN DEVELOPMENT 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=18 g=1 f=1 -->

<USBUREAU>Office of the Secretary</USBUREAU>
<!-- PJG /ITAG -->

<!-- PJG ITAG l=52 g=1 f=1 -->

<CFRNO>24 CFR Parts 25 and 26 </CFRNO>
