<!-- PJG ITAG l=74 g=1 f=1 -->
SGA/DAA 94&hyph;24, Local Learning Laboratory 
<!-- PJG 0012 frnewline -->
Budget Detail Sheet
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG /STAG -->

<!-- PJG STAG 4706 -->

<!-- PJG 0012 frnewline -->

<!-- PJG ITAG l=77 g=1 f=1 -->
Applicant:
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<TABLE>

<!-- PJG tableformat 3,L2,tp0,i1,s100,10,10 -->

<!-- PJG 0012 frnewline -->

<!-- PJG ITAG l=95 g=1 f=1 -->
