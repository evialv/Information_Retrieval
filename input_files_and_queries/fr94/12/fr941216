<!-- PJG /ITAG -->

</FOOTNAME>
<!-- PJG ITAG l=28 g=1 f=1 -->
Copies may be obtained from Van Nostrand Reinhold Company, Inc., 135 West 50th Street, New York, NY 10020.
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->
</FOOTNOTE>

<!-- PJG /STAG -->
</TEXT>
</DOC>



<DOC>
<DOCNO> FR941216-1-00125 </DOCNO>
<PARENT> FR941216-1-00012 </PARENT>
