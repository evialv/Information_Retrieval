<!-- PJG 0012 frnewline -->
6f, 6g, 6h, 6i, 6j, 6k, 6l, 6m, 6n, 6o, 6p, 7, 7a, 7b, 8, 9, 12, 
<!-- PJG 0012 frnewline -->
