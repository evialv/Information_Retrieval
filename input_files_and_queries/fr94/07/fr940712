<!-- PJG ITAG l=80 g=1 f=3 -->
&sect;675.22 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=89 g=1 f=3 -->
Time and area closures. 
<!-- PJG 0012 frnewline -->

