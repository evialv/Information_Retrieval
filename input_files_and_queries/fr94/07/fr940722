<USDEPT>NATIONAL SCIENCE FOUNDATION</USDEPT>

<!-- PJG /ITAG -->

<!-- PJG ITAG l=52 g=1 f=1 -->

<CFRNO>45 CFR Parts 606, 610, 611, 612, 613, 614, 630, 640, 641, 650, 671, 672, and 689 </CFRNO>
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

