<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
: 20,000.
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
