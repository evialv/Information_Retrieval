<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
 Modification No. 1 to scientific research permit no. 918 (P191E).
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=59 g=1 f=1 -->

<!-- PJG 0012 frnewline -->

