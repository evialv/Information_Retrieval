<!-- PJG 0012 frnewline -->

<!-- PJG ITAG l=21 g=1 f=1 -->
Dated: July 18, 1994. 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->
</SUPPLEM>
