<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
 Mr. Stanley Hanna, Social 
<!-- PJG 0012 frnewline -->
Insurance Specialist, 3&hyph;D&hyph;1 Operations Building, 6401 Security 
<!-- PJG 0012 frnewline -->
Boulevard, Baltimore, MD 21235, telephone (410) 966&hyph;7077. 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->
