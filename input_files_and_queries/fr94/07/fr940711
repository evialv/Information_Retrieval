<!-- PJG /ITAG -->

<!-- PJG ITAG l=90 g=1 f=4 -->
Federal Register
<!-- PJG /ITAG -->

<!-- PJG ITAG l=90 g=1 f=1 -->
&blank;/&blank;Vol. 59, No. 131&blank;/&blank;Monday, 
<!-- PJG 0012 frnewline -->
July 11, 1994&blank;/&blank;Rules and Regulations 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=52 g=1 f=1 -->

<USDEPT>DEPARTMENT OF JUSTICE </USDEPT>

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=18 g=1 f=1 -->

<USBUREAU>Bureau of Prisons</USBUREAU>
<!-- PJG /ITAG -->

<!-- PJG ITAG l=52 g=1 f=1 -->
28 CFR Part 571 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

