<!-- PJG /ITAG -->

<!-- PJG ITAG l=50 g=1 f=1 -->


<USDEPT>COMMITTEE FOR THE IMPLEMENTATION OF TEXTILE AGREEMENTS</USDEPT>

<!-- PJG /ITAG -->

<!-- PJG ITAG l=55 g=1 f=1 -->

<DOCTITLE>New Transshipment Charges for Certain Cotton Textile Products Produced or Manufactured in the People's Republic
of China</DOCTITLE>
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

