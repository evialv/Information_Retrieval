<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
 (propylene) + 2 O
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=5 f=2 -->
2
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
 (oxygen) __> C
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=5 f=2 -->
