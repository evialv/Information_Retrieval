15. Medina Power Company
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=71 g=1 f=1 -->
[Docket No. QF91&hyph;040&hyph;000] 
