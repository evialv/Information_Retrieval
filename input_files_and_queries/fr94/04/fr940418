<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
H
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=5 f=2 -->
10
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
O (diphenyl oxide) + 2 C
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=5 f=2 -->
3
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
H
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=5 f=2 -->
