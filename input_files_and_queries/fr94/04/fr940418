<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=1 f=1 -->
 May 12&hyph;13, 1994 8:30 a.m.&hyph;5 p.m. 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=1 f=3 -->
Place:
<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=1 f=1 -->
