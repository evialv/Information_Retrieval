5. Ocean State Power II 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=71 g=1 f=1 -->
[Docket No. ER94&hyph;1067&hyph;000] 
<!-- PJG 0012 frnewline -->

