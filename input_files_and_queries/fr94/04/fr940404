<USDEPT>DEPARTMENT OF STATE</USDEPT>

<!-- PJG /ITAG -->

<!-- PJG ITAG l=18 g=1 f=1 -->

<USBUREAU>Bureau of Political-Military Affairs</USBUREAU>
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=52 g=1 f=1 -->

<CFRNO>22 CFR Part 126</CFRNO>
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

