<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=74 g=1 f=3 -->
ANM MT D Helena, MT [Revised]
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=26 g=1 f=3 -->
Helena Regional Airport, MT
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=24 g=1 f=3 -->
(lat. 46
<!-- PJG 0017 degreeslatlong -->
&cir;36
<!-- PJG 0020 minuteslatlong -->
&rsquo;25
<!-- PJG 0020 minuteslatlong -->
