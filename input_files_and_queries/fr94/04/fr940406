<!-- PJG 0012 frnewline -->
Frederick
<!-- PJG 0012 frnewline -->
Greensville
<!-- PJG 0012 frnewline -->
Halifax
<!-- PJG 0012 frnewline -->
Henry
<!-- PJG 0012 frnewline -->
Montgomery
<!-- PJG 0012 frnewline -->
Rockbridge
<!-- PJG 0012 frnewline -->
Rockingham
<!-- PJG 0012 frnewline -->
Southhampton
<!-- PJG 0012 frnewline -->
Wise
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=16 g=1 f=3 -->
