<USBUREAU>Agricultural Marketing Service</USBUREAU>
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=52 g=1 f=1 -->
7 CFR Part 110
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=41 g=1 f=1 -->

<RINDOCK>[SD&hyph;94&hyph;001 PR]</RINDOCK>
<!-- PJG 0012 frnewline -->

