<!-- PJG tableformat 5,L2,tp0,i1,s100,xls36,10,10,10 -->

<!-- PJG ITAG l=95 g=1 f=1 -->
&blank;
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG 007 0150 intable -->
1HUD RDD Surveys with increases
<!-- PJG 0012 frnewline -->

<!-- PJG 007 0150 intable -->
1State
<!-- PJG 0012 frnewline -->

<!-- PJG 007 0150 intable -->
