<!-- PJG 007 0104 intable -->
(1,170) 
<!-- PJG 0012 frnewline -->
FY 1994 Adjusted Total Appropriation 
<!-- PJG 007 0104 intable -->
$3,502,056,732 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
