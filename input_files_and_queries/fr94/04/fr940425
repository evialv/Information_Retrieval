<DOCTITLE>Jones Tire &amp; Battery Site; Notice of Proposed Settlement</DOCTITLE>
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<AGENCY>
<!-- PJG ITAG l=10 g=1 f=2 -->
AGENCY: 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
Environmental Protection Agency.
<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
<!-- PJG /QTAG -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
