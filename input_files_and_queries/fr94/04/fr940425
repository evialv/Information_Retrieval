<!-- PJG ITAG l=55 g=1 f=1 -->
Partial Revocation of Public Land Order No. 1564; Idaho
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<AGENCY>
<!-- PJG ITAG l=10 g=1 f=2 -->
