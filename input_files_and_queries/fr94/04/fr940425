<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
 0901 UTC, June 23, 1994. 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
