25 CFR Part 248
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
Use of Columbia River In-Lieu Fishing Sites; Final Rule
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG /STAG -->

<!-- PJG /FTAG -->

<!-- PJG FTAG 4701 -->

<!-- PJG STAG 4701 -->

<!-- PJG ITAG l=90 g=1 f=1 -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=90 g=1 f=4 -->
Federal Register
<!-- PJG /ITAG -->

<!-- PJG ITAG l=90 g=1 f=1 -->
&blank;/&blank;Vol. 59, No. 67&blank;/&blank;Thursday, April 7, 1994&blank;/&blank;Rules and Regulations
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=52 g=1 f=1 -->

<USDEPT>DEPARTMENT OF THE INTERIOR</USDEPT>

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=18 g=1 f=1 -->

<USBUREAU>Bureau of Indian Affairs</USBUREAU>
