<!-- PJG 007 0104 intable -->
15 
<!-- PJG 007 0104 intable -->
15 
<!-- PJG 007 0104 intable -->
9 
<!-- PJG 007 0104 intable -->
1.35 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
