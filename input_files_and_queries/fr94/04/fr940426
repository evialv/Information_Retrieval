<!-- PJG ITAG l=28 g=1 f=3 -->
See 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=28 g=1 f=1 -->
Section I.D 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=28 g=1 f=3 -->
supra. 
<!-- PJG 0012 frnewline -->

