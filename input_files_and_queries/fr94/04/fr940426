<!-- PJG ITAG l=55 g=1 f=1 -->
Section 8 Housing Assistance Payments Program_Contract Rent Annual Adjustment Factors 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<AGENCY>
<!-- PJG ITAG l=10 g=1 f=2 -->
