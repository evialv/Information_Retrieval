<USDEPT>FARM CREDIT ADMINISTRATION</USDEPT>

<!-- PJG /ITAG -->

<!-- PJG ITAG l=52 g=1 f=4 -->

<CFRNO>12 CFR Parts 600, 604, 605, 611, and 615 </CFRNO>
<!-- PJG 0012 frnewline -->

