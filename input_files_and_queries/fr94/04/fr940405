<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=4 -->
Thermal conditioning:
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=03 g=1 f=4 -->
