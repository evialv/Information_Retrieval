<!-- PJG 007 0104 intable -->
0.405 
<!-- PJG 0012 frnewline -->
7 
<!-- PJG 007 0104 intable -->
0.347 
<!-- PJG 0012 frnewline -->

<!-- PJG 007 0146 intable -->
