<!-- PJG /ITAG -->

<!-- PJG ITAG l=26 g=1 f=1 -->
T. 14 S., R. 69 E.,
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=24 g=1 f=1 -->
Sec. 32, SE1/4SW1/4, SW1/4SE1/4.
<!-- PJG 0012 frnewline -->
