<!-- PJG /ITAG -->

</FOOTNAME>
<!-- PJG ITAG l=28 g=1 f=1 -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=28 g=1 f=3 -->
Supra 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=28 g=1 f=1 -->
note 3. 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->
</FOOTNOTE>

<FOOTNOTE>
<FOOTNAME>
