<!-- PJG STAG 4702 -->

<!-- PJG ITAG l=11 g=1 f=1 -->
Hazard classes used in this part are defined in the following sections of 49 CFR: 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<TABLE>

<!-- PJG tableformat 2,L2,i1,s100,9  -->

<!-- PJG 0012 frnewline -->

<!-- PJG ITAG l=95 g=1 f=1 -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=95 g=1 f=4 -->
Hazard Class Definitions 
<!-- PJG 0012 frnewline -->

<!-- PJG 007 0150 intable -->
1Class No._description 
<!-- PJG 007 0150 intable -->
1Reference
<!-- PJG 0012 frnewline -->

<!-- PJG 007 0152 intable -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=01 g=1 f=4 -->
Class 1, 1.1, 1.4, 1.5_Explosives 
<!-- PJG 007 0104 intable -->
&sect;173.50
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
Class 2_Compressed gas 
<!-- PJG 007 0104 intable -->
&sect;173.115 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
Class 3_Flammable liquid 
<!-- PJG 007 0104 intable -->
&sect;173.120 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
Class 4, 4.1, 4.2, 4.3_Flammable solid, spontaneously combustible 
<!-- PJG 0012 frnewline -->
material, dangerous when wet material 
<!-- PJG 007 0104 intable -->
&sect;173.120 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
Class 5, 5.1_Oxidizer and organic peroxide 
<!-- PJG 007 0104 intable -->
&sect;173.127 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
