<!-- PJG STAG 4700 -->

<TABLE>

<!-- PJG tableformat 4,L2,i1,s150,10,10,10 -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG ITAG l=95 g=1 f=4 -->
Table 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=95 g=1 f=1 -->
2._
<!-- PJG /ITAG -->

<!-- PJG ITAG l=95 g=1 f=4 -->
