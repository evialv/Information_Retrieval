<DOCNO> FR940414-1-00071 </DOCNO>
<PARENT> FR940414-1-00010 </PARENT>
<TEXT>
 
<!-- PJG STAG 4702 -->

<TABLE>

<!-- PJG tableformat 5,L2,i1,s100,15,15,15,15 -->

<!-- PJG 0012 frnewline -->

