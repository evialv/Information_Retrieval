<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
. The conditions of use specified in paragraph (d) of this section were NAS/NRC reviewed and found effective.
<!-- PJG 0012 frnewline -->
(d) 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
