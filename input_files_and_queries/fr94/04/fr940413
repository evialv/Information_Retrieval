<!-- PJG STAG 4706 -->

<!-- PJG ITAG l=11 g=1 f=1 -->
4. Shortfall in Both, With XM Shortfall Exceeding Non-XM Shortfall:
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<TABLE>

<!-- PJG tableformat 4,L2,tp0,i1,s100,15,15,10 -->

<!-- PJG 0012 frnewline -->

<!-- PJG ITAG l=95 g=1 f=1 -->
&blank;
<!-- PJG 0012 frnewline -->

<!-- PJG 007 0150 intable -->
1&blank; 
<!-- PJG 007 0150 intable -->
