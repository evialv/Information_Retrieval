<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
&blank;&blank;3928 divided by 3 
<!-- PJG 007 0104 intable -->
1309.3 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=01 g=1 f=1 -->
