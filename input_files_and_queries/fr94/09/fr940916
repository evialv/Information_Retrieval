<!-- PJG ITAG l=59 g=1 f=1 -->

<!-- PJG 0012 frnewline -->
 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->
</ACTION>
<SUMMARY>
<!-- PJG ITAG l=10 g=1 f=2 -->
SUMMARY:
<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
 This action adds to the Procurement List a service to be 
<!-- PJG 0012 frnewline -->
furnished by nonprofit agencies employing persons who are blind or 
<!-- PJG 0012 frnewline -->
have other severe disabilities. 
<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
<!-- PJG /QTAG -->

