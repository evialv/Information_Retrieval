<!-- PJG /ITAG -->
</BILLING>

<!-- PJG 0012 frnewline -->

<!-- PJG 0101 clearpage -->

<!-- PJG 0012 frnewline -->

<!-- PJG /STAG -->

<!-- PJG STAG 4706 -->

<!-- PJG ITAG l=95 g=1 f=4 -->
&blank;&blank;&blank;&blank;&blank;&blank;&blank;&blank;&blank;&blank;&blank;&blank;&blank;&blank;&blank;&blank;&blank;&blank;TABLE
2_EXAMPLE OF HIGH UTILIZATION CONVENTIONAL FREIGHT CAR
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG /STAG -->

