<!-- PJG ITAG l=52 g=1 f=1 -->
Subpart D_Subsistence Taking of Fish and Wildlife 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=80 g=1 f=1 -->
