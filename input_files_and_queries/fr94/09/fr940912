<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
1. International Bancshares Corporation
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
, Laredo, Texas; to engage 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
de novo
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
