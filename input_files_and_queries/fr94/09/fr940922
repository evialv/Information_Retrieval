<!-- PJG /ITAG -->

<!-- PJG ITAG l=84 g=1 f=1 -->
Reinsured Mortgages
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=80 g=1 f=1 -->
&sect;207.270
<!-- PJG /ITAG -->

<!-- PJG ITAG l=89 g=1 f=1 -->
Special Reinsurance Provisions.
