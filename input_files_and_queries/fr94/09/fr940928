<!-- PJG ITAG l=80 g=1 f=1 -->
&sect;286.5(g)
<!-- PJG /ITAG -->

<!-- PJG ITAG l=89 g=1 f=1 -->
 [Amended]
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

