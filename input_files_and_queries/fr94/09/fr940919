<!-- PJG /ITAG -->

</FOOTNAME>
<!-- PJG ITAG l=28 g=1 f=1 -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=28 g=1 f=3 -->
See 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=28 g=1 f=1 -->
Hatfield Associates, Inc., 
<!-- PJG /ITAG -->

