<!-- PJG 007 0162 intable -->
n,s
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=04 g=1 f=4 -->
Total Emissions 
<!-- PJG 007 0104 intable -->
218,226 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

</TABLE>

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<TABLE>

<!-- PJG tableformat 2,L2,i1,s10,10 -->

