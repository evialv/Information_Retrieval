<!-- PJG STAG 4700 -->

<TABLE>

<!-- PJG tableformat 2,L2,i1,s50,11 -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG ITAG l=95 g=1 f=1 -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=95 g=1 f=4 -->
