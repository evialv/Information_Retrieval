<!-- PJG 0012 frnewline -->
*Total lead, 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=01 g=6 f=1 -->
m
<!-- PJG /ITAG -->

<!-- PJG ITAG l=01 g=1 f=1 -->
g/l 
<!-- PJG 007 0104 intable -->
 
