3.57
<!-- PJG 0012 frnewline -->
No later than December 31, 1993 
<!-- PJG 007 0104 intable -->
3.13 
<!-- PJG 007 0104 intable -->
3.13
<!-- PJG 0012 frnewline -->
No later than December 31, 1995 
<!-- PJG 007 0104 intable -->
2.7 
<!-- PJG 007 0104 intable -->
3.19
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=22 g=1 f=1 -->
Unit 4:
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=02 g=1 f=1 -->
Beginning January 1, 1992 
<!-- PJG 007 0104 intable -->
3.57 
<!-- PJG 007 0104 intable -->
3.57
<!-- PJG 0012 frnewline -->
No later than December 31, 1993 
<!-- PJG 007 0104 intable -->
3.13 
<!-- PJG 007 0104 intable -->
3.13
<!-- PJG 0012 frnewline -->
No later than December 31, 1995 
<!-- PJG 007 0104 intable -->
2.7 
<!-- PJG 007 0104 intable -->
0.60
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=22 g=1 f=1 -->
