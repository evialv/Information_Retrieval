<!-- PJG STAG 4703 -->

<!-- PJG ITAG l=69 g=1 f=1 -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=18 g=1 f=1 -->

<USBUREAU>Minerals Management Service</USBUREAU>
<!-- PJG /ITAG -->

<!-- PJG ITAG l=55 g=1 f=1 -->

