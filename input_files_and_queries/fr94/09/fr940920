<!-- PJG STAG 4702 -->

<!-- PJG ITAG l=56 g=1 f=1 -->
PART 152_STATE GRANTS FOR ARSON RESEARCH, PREVENTION, AND CONTROL 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=52 g=1 f=1 -->
Subpart A_Purpose, Scope, Definitions 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=20 g=1 f=1 -->
Sec. 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=26 g=1 f=1 -->
152.1&blank;Purpose. 
<!-- PJG 0012 frnewline -->
152.2&blank;Scope. 
<!-- PJG 0012 frnewline -->
