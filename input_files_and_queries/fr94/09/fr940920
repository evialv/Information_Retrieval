<!-- PJG 007 0146 intable -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=03 g=5 f=1 -->
g
<!-- PJG /ITAG -->

<!-- PJG ITAG l=03 g=1 f=1 -->
SNCR = selective noncatalytic reduction. 
<!-- PJG 0012 frnewline -->

<!-- PJG 007 0146 intable -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=03 g=5 f=1 -->
h
<!-- PJG /ITAG -->

<!-- PJG ITAG l=03 g=1 f=1 -->
Does not add to 100 percent due to rounding. 
<!-- PJG 0012 frnewline -->

<!-- PJG 007 0146 intable -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=03 g=5 f=1 -->
i
<!-- PJG /ITAG -->

<!-- PJG ITAG l=03 g=1 f=1 -->
MOD/SA = modular starved-air combustor. 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

</TABLE>

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /STAG -->
