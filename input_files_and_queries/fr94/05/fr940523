<!-- PJG /ITAG -->

<!-- PJG ITAG l=55 g=1 f=1 -->
Cable Television Service; List of Major Television Markets
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<AGENCY>
<!-- PJG ITAG l=10 g=1 f=2 -->
agency: 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
Federal Communications Commission.
<!-- PJG /ITAG -->

