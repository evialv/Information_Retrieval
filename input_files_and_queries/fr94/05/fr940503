<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
$400,000. 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
Estimated Range of Awards: 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
$180,000&hyph;$210,000. 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
Estimated Average Size of Awards: 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
$200,000. 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
Estimated Number of Awards: 
<!-- PJG /ITAG -->

