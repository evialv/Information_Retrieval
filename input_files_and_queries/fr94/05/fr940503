<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
, Americus, Georgia; to engage 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
de novo
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
