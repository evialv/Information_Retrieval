<!-- PJG STAG 4717 -->

<!-- PJG ITAG l=01 g=1 f=1 -->
Tuesday
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=02 g=1 f=1 -->
May 3, 1994
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=09 g=1 f=1 -->
 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=03 g=1 f=1 -->
Part VIII
