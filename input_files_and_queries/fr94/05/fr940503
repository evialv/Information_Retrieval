<USDEPT>USDA_ASCS</USDEPT>

<!-- PJG /ITAG -->

<!-- PJG ITAG l=60 g=1 f=4 -->
Proposed Rule Stage 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=62 g=1 f=4 -->
&blank; 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

