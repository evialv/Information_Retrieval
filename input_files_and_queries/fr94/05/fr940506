<!-- PJG /ITAG -->

<!-- PJG ITAG l=55 g=1 f=1 -->
Rules and Regulations Under the Wool Products Labeling Act of 1939
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<AGENCY>
<!-- PJG ITAG l=10 g=1 f=2 -->
AGENCY: 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
Federal Trade Commission.
<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
