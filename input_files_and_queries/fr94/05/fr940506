<!-- PJG /ITAG -->

<!-- PJG ITAG l=91 g=1 f=1 -->

<RINDOCK>RIN 2115&hyph;AE37</RINDOCK>
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=55 g=1 f=1 -->
Expanded Hull Identification Number and New Requirements for Certificates of Origin
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<AGENCY>
