<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=80 g=1 f=1 -->
&sect;887.251 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=89 g=1 f=1 -->
Housing quality standards (HQS). 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=37 g=1 f=1 -->
