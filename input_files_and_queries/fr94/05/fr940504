<!-- PJG 007 0150 intable -->
1Schedule
<!-- PJG 007 0152 intable -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=01 g=1 f=1 -->
Coca Leaves (9040)
<!-- PJG 007 0104 intable -->
II
<!-- PJG 0012 frnewline -->
Opium, raw (9600)
<!-- PJG 007 0104 intable -->
