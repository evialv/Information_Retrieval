<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
(6) 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
Exception for well-capitalized bank holding companies. 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
