<!-- PJG STAG 4700 -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<TABLE>

<!-- PJG tableformat 3,L2,tp0,i1,s100,xls160,xls160 -->

<!-- PJG ITAG l=95 g=1 f=1 -->
&blank;
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG 007 0150 intable -->
1Point 
<!-- PJG 007 0150 intable -->
1Latitude 
<!-- PJG 007 0150 intable -->
1Longitude
<!-- PJG 007 0152 intable -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=01 g=1 f=1 -->
