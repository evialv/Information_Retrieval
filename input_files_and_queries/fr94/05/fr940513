<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
This regulation is effective from 8:30 a.m. to 5 p.m., on July 16, 1994.
<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
<!-- PJG /QTAG -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
</DATE>
<FURTHER>
<!-- PJG ITAG l=10 g=1 f=2 -->
