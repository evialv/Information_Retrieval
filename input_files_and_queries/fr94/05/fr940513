244
<!-- PJG /ITAG -->

</FOOTNAME>
<!-- PJG ITAG l=28 g=1 f=1 -->
Osram (Tr.), 328. 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->
</FOOTNOTE>

<FOOTNOTE>
<FOOTNAME>
<!-- PJG ITAG l=28 g=1 f=1 -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=28 g=5 f=1 -->
245
<!-- PJG /ITAG -->

</FOOTNAME>
<!-- PJG ITAG l=28 g=1 f=1 -->
Philips, G&hyph;5, 3; NEMA, G&hyph;3, 27, 32, (Tr.), 327. 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

