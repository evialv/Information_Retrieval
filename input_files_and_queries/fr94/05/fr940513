<!-- PJG ITAG l=80 g=1 f=1 -->
&sect;305.2 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=89 g=1 f=1 -->
Definitions. 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

