<!-- PJG ITAG l=50 g=1 f=1 -->
DEPARTMENT OF AGRICULTURE 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=18 g=1 f=1 -->
Animal and Plant Health Inspection Service
<!-- PJG /ITAG -->

