<!-- PJG STAG 4703 -->

<!-- PJG ITAG l=73 g=1 f=1 -->
12. Estimate of Costs
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<TABLE>

<!-- PJG tableformat 2,L0,tp0,p0,8/9,g1,t1,i1,s25,9  -->

<!-- PJG ITAG l=95 g=1 f=1 -->
&blank;
<!-- PJG 0012 frnewline -->

<!-- PJG 007 0150 intable -->
1&blank;
<!-- PJG 007 0150 intable -->
1&blank;
<!-- PJG 007 0152 intable -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=1 f=1 -->
For HUD Section 3 Reporting Form_Preparing the Report
