<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=3 -->
Effective date:
<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
 0901 u.t.c. June 23, 1994.
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
Comment date:
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
 Comments must be received on or before June 23, 1994.
<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
