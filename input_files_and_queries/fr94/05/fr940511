<!-- PJG ITAG l=55 g=1 f=3 -->
Establishment of Class E Airspace Areas
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<AGENCY>
<!-- PJG ITAG l=10 g=1 f=2 -->
AGENCY: 
