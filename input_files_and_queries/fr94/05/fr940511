<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
 Chris Oliver, Deputy Director, North Pacific Fishery Management Council, P.O. Box 103136, Anchorage, AK 99510;
telephone: (907) 271&hyph;2809.
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
<!-- PJG /QTAG -->

<!-- PJG 0012 frnewline -->
</FURTHER>
