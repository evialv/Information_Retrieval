<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=56 g=1 f=1 -->
PART 925_OLYMPIC COAST NATIONAL MARINE SANCTUARY
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=20 g=1 f=1 -->
Sec.
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=26 g=1 f=1 -->
925.1&blank;Purpose.
<!-- PJG 0012 frnewline -->
925.2&blank;Boundary.
<!-- PJG 0012 frnewline -->
925.3&blank;Definitions.
<!-- PJG 0012 frnewline -->
925.4&blank;Allowed activities.
<!-- PJG 0012 frnewline -->
925.5&blank;Prohibited activities.
<!-- PJG 0012 frnewline -->
925.6&blank;Emergency regulations.
<!-- PJG 0012 frnewline -->
