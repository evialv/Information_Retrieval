<!-- PJG ITAG l=41 g=1 f=1 -->

<RINDOCK>[PF&hyph;596; FRL&hyph;4775&hyph;1] </RINDOCK>
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=52 g=1 f=1 -->

<DOCTITLE>Pesticide Tolerance Petitions </DOCTITLE>
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
<!-- PJG /QTAG -->

<!-- PJG 0012 frnewline -->

<AGENCY>
<!-- PJG ITAG l=10 g=1 f=2 -->
AGENCY:
