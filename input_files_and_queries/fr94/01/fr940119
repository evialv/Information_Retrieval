Federal Register
<!-- PJG /ITAG -->

<!-- PJG ITAG l=90 g=1 f=1 -->
&blank;/&blank;Vol. 59, No. 12&blank;/&blank;Wednesday, January 19, 1994&blank;/&blank;Notices
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG /STAG -->

<!-- PJG /FTAG -->

<!-- PJG STAG 4703 -->

<!-- PJG ITAG l=52 g=1 f=1 -->

<USDEPT>ENVIRONMENTAL PROTECTION AGENCY</USDEPT>

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<USDEPT></USDEPT>

<!-- PJG 0012 frnewline -->
DEPARTMENT OF AGRICULTURE
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=18 g=1 f=1 -->

<USBUREAU>Soil Conservation Service</USBUREAU>
