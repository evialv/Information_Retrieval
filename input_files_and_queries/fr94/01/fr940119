<!-- PJG /ITAG -->

<!-- PJG ITAG l=15 g=1 f=1 -->
$3.00
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=15 g=1 f=3 -->
Actual charge effective date: 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=15 g=1 f=1 -->
December 1, 1992
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=15 g=1 f=3 -->
