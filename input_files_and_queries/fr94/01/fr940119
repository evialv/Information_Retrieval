<!-- PJG 007 0150 intable -->
1Twelve-month restraint limit
<!-- PJG 007 0152 intable -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG ITAG l=01 g=1 f=3 -->
315
<!-- PJG 007 0104 intable -->
&hyph;0&hyph;.
<!-- PJG 0012 frnewline -->
