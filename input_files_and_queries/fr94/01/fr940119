<!-- PJG STAG 4703 -->

<!-- PJG ITAG l=11 g=1 f=1 -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
Application Type:
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
 Impose and Use PFC Revenue.
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
PFC Level:
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
 $3.00.
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
Total Approved PFC Revenue:
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
 $5,540,745.
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
