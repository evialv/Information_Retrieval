<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=24 g=1 f=1 -->

<!-- PJG 0012 frnewline -->
Sections 31 through 33, T7N, R19E 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
Sections 1, 2, 10, 11, and 12, T6N, R18E 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
Sections 4 through 9, T6N, R19E
<!-- PJG /ITAG -->

<!-- PJG QTAG 04 -->
<!-- PJG /QTAG -->

