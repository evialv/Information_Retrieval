<!-- PJG 0012 frnewline -->
(line 8 minus line 11) 
<!-- PJG 007 0104 intable -->
6,200 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=22 g=1 f=1 -->

