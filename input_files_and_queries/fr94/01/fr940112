<!-- PJG STAG 4702 -->

<!-- PJG ITAG l=69 g=1 f=1 -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=52 g=1 f=1 -->

<CFRNO>14 CFR Part 71</CFRNO>
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

