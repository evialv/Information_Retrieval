<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
liter_L or l 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
milliliter_mL or ml 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
square decimeter_dm2 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
