<!-- PJG 007 0152 intable -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=01 g=1 f=1 -->
Kilo- 
<!-- PJG 007 0104 intable -->
k 
<!-- PJG 007 0104 intable -->
x 103 
<!-- PJG 0012 frnewline -->
Deca- 
<!-- PJG 007 0104 intable -->
da 
<!-- PJG 007 0104 intable -->
x 10 
<!-- PJG 0012 frnewline -->
Deci- 
<!-- PJG 007 0104 intable -->
d 
<!-- PJG 007 0104 intable -->
x 10^1 
<!-- PJG 0012 frnewline -->
