<!-- PJG /ITAG -->

<!-- PJG ITAG l=28 g=1 f=1 -->
4 I.C.C.2d 164 (1987). 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->
</FOOTNOTE>

<FOOTNOTE>
<FOOTNAME>
<!-- PJG ITAG l=28 g=1 f=1 -->

