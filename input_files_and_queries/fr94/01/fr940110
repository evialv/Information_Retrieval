<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=03 g=1 f=4 -->
Breakfast
<!-- PJG 0012 frnewline -->

<!-- PJG 007 0104 intable -->
.0850
<!-- PJG 0012 frnewline -->
