<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
(b) *&blank;*&blank;* 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
Employment and training agency 
<!-- PJG /ITAG -->

