<USDEPT>PHYSICIAN PAYMENT REVIEW COMMISSION</USDEPT>

<!-- PJG /ITAG -->

<!-- PJG ITAG l=52 g=1 f=1 -->

<DOCTITLE>Meeting</DOCTITLE>
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<AGENCY>
<!-- PJG ITAG l=10 g=1 f=2 -->
AGENCY: 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
Physician Payment Review Commission.
<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
<!-- PJG /QTAG -->

<!-- PJG 0012 frnewline -->
</AGENCY>
<ACTION>
<!-- PJG ITAG l=10 g=1 f=2 -->
