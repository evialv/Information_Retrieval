<!-- PJG 007 0152 intable -->
 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=01 g=1 f=1 -->
Settleable Solids 
<!-- PJG 007 0104 intable -->
0.2 ml/L. 
<!-- PJG 0012 frnewline -->
