<!-- PJG ITAG l=56 g=1 f=1 -->

<DOCTITLE>Prospective Grant of Exclusive License: Cell Stress Transcriptional Factors</DOCTITLE>
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<AGENCY>
<!-- PJG ITAG l=10 g=1 f=2 -->
AGENCY: 
<!-- PJG /ITAG -->

