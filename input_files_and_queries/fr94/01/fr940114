<!-- PJG ITAG l=84 g=1 f=1 -->
1. NUI Corporation Pennsylvania &amp; Southern Gas Company
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=71 g=1 f=1 -->
[Docket No. CP94&hyph;156&hyph;000]
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=44 g=1 f=1 -->
January 5, 1994.
<!-- PJG 0012 frnewline -->

