<!-- PJG ITAG l=52 g=1 f=1 -->
Radio Broadcasting Services; Islesboro and Winter Harbor, ME
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<AGENCY>
<!-- PJG ITAG l=10 g=1 f=2 -->
