<!-- PJG /ITAG -->

<!-- PJG ITAG l=52 g=1 f=1 -->
PART 336_DURATION OF NORMAL AND EXTENDED BENEFITS 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

