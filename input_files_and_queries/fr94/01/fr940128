<!-- PJG STAG 4703 -->

<TABLE>

<!-- PJG tableformat 3,L2(0,,),tp0,p6,8/9,i1,s50,r50,r100 -->

<!-- PJG 0012 frnewline -->

<!-- PJG ITAG l=95 g=1 f=1 -->
&blank;
<!-- PJG 007 0150 intable -->
1&blank;
<!-- PJG 007 0150 intable -->
1&blank;
<!-- PJG 007 0150 intable -->
