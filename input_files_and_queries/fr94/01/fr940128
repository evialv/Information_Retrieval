<DOCTITLE>Government Auditing Standards Advisory Council; Meeting</DOCTITLE>
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG /STAG -->

<!-- PJG STAG 4703 -->

<AGENCY>
<!-- PJG ITAG l=10 g=1 f=2 -->
