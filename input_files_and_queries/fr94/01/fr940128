<!-- PJG 0012 frnewline -->

<!-- PJG 007 0152 intable -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=01 g=1 f=1 -->
266.1 
<!-- PJG 007 0104 intable -->
Removed. 
<!-- PJG 0012 frnewline -->
266.2 
<!-- PJG 007 0104 intable -->
266.1(b). 
<!-- PJG 0012 frnewline -->
266.3 
<!-- PJG 007 0104 intable -->
266.3(a). 
<!-- PJG 0012 frnewline -->
266.4 
<!-- PJG 007 0104 intable -->
266.12. 
<!-- PJG 0012 frnewline -->
266.5 
<!-- PJG 007 0104 intable -->
266.2 and 266.5. 
<!-- PJG 0012 frnewline -->
266.6 
