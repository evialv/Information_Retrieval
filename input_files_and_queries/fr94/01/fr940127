<USDEPT>DEPARTMENT OF AGRICULTURE</USDEPT>

<!-- PJG /ITAG -->

<!-- PJG ITAG l=18 g=1 f=1 -->

<USBUREAU>Agricultural Stabilization and Conservation Service</USBUREAU>
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=52 g=1 f=1 -->

<CFRNO>7 CFR Part 703 </CFRNO>
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=91 g=1 f=1 -->

<RINDOCK>RIN 0560&hyph;AD59 </RINDOCK>
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG /STAG -->

<!-- PJG STAG 4704 -->

<!-- PJG ITAG l=55 g=1 f=1 -->
Wetlands Reserve Program 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG /STAG -->

<!-- PJG STAG 4700 -->

<AGENCY>
<!-- PJG ITAG l=10 g=1 f=2 -->
AGENCY:
<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
 Agricultural Stabilization and Conservation Service, USDA. 
<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
