<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
M
<!-- PJG /ITAG -->

<!-- PJG ITAG l=16 g=5 f=2 -->
3
<!-- PJG /ITAG -->

<!-- PJG ITAG l=16 g=1 f=1 -->
 = ^1.613493 + (0.0319584 * Avg Water Temp, 
<!-- PJG 0040 degreeslatlong -->
&cir;F, at Freeport, CA), r
<!-- PJG /ITAG -->

<!-- PJG ITAG l=16 g=5 f=1 -->
2
<!-- PJG /ITAG -->

<!-- PJG ITAG l=16 g=1 f=1 -->
 = .32
<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
<!-- PJG /QTAG -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG ITAG l=11 g=1 f=1 -->
