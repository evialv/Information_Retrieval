<!-- PJG /ITAG -->

<!-- PJG ITAG l=14 g=1 f=1 -->
57.
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=14 g=1 f=3 -->
Burden Hours: 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=14 g=1 f=1 -->
1,140.
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=16 g=1 f=1 -->

