<!-- PJG tableformat 4,L2,i1,s20,9,9,9 -->

<!-- PJG ITAG l=95 g=1 f=4 -->

<!-- PJG 0012 frnewline -->
Table 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=95 g=1 f=1 -->
2._
<!-- PJG /ITAG -->

<!-- PJG ITAG l=95 g=1 f=4 -->
Hospital-Based HHAs At or Over the Cost Limits 
<!-- PJG 0012 frnewline -->

