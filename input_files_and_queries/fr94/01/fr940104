<!-- PJG STAG 4702 -->

<!-- PJG ITAG l=71 g=1 f=1 -->
(3) The Transportation Discounted Rate Record 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<TABLE>

<!-- PJG tableformat 5,L2,tp0,i1,xs45,xs150,10,xs45,r100 -->

