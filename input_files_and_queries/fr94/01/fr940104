<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
February 23, 1994, 7 p.m. until 9 p.m.
<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
<!-- PJG /QTAG -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
</DATE>
