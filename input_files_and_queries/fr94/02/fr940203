<DOCNO> FR940203-2-00010 </DOCNO>
<PARENT> FR940203-2-00006 </PARENT>
<TEXT>
 
<!-- PJG STAG 4703 -->

<!-- PJG ITAG l=69 g=1 f=1 -->

<!-- PJG /ITAG -->

