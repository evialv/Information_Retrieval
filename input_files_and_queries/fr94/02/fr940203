<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=01 g=1 f=1 -->
Town of Rollingsford
<!-- PJG 007 0104 intable -->
$100
<!-- PJG 007 0104 intable -->
$1,697 
<!-- PJG 0012 frnewline -->
City of Marshall 
<!-- PJG 007 0104 intable -->
22 
<!-- PJG 007 0104 intable -->
367 
<!-- PJG 0012 frnewline -->
