<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
1,800; 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=3 -->
Average Hours Per Response: 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
