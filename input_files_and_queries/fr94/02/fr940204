<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

</IMPORT>

<!-- PJG /STAG -->

<!-- PJG STAG 4702 -->

<!-- PJG ITAG l=16 g=1 f=1 -->
where: 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
<!-- PJG /QTAG -->

<!-- PJG 0012 frnewline -->

<!-- PJG ITAG l=16 g=1 f=1 -->
DPF = driving pattern factor 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
n
<!-- PJG /ITAG -->

<!-- PJG ITAG l=16 g=5 f=2 -->
t
<!-- PJG /ITAG -->

