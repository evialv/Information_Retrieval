<DOCTITLE>ABT Money Market Series, Inc.; Application for Deregistration</DOCTITLE>
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=44 g=1 f=1 -->
January 27, 1994.
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<AGENCY>
<!-- PJG ITAG l=10 g=1 f=2 -->
AGENCY:
<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
