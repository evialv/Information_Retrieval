<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=1 f=1 -->
1000&hyph;1600 (classified).
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=21 g=1 f=3 -->
Place: 
<!-- PJG /ITAG -->

