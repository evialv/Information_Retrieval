<!-- PJG /ITAG -->

<!-- PJG ITAG l=24 g=1 f=1 -->
=normalizing factor of 20 MByte/s. 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
S
<!-- PJG /ITAG -->

