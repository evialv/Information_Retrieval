<!-- PJG /ITAG -->

<!-- PJG ITAG l=26 g=1 f=1 -->
 " TP
<!-- PJG /ITAG -->

<!-- PJG ITAG l=26 g=5 f=2 -->
n
<!-- PJG /ITAG -->

<!-- PJG ITAG l=26 g=1 f=1 -->
, and
<!-- PJG 0012 frnewline -->
in the case of TP
<!-- PJG /ITAG -->

<!-- PJG ITAG l=26 g=5 f=2 -->
i
<!-- PJG /ITAG -->

<!-- PJG ITAG l=26 g=1 f=1 -->
=TP
<!-- PJG /ITAG -->

<!-- PJG ITAG l=26 g=5 f=2 -->
i+1
<!-- PJG /ITAG -->

<!-- PJG ITAG l=26 g=1 f=1 -->
, from the largest to smallest; i.e.: 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->
C
<!-- PJG /ITAG -->

<!-- PJG ITAG l=26 g=5 f=2 -->
i 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=26 g=1 f=1 -->
" C
<!-- PJG /ITAG -->

<!-- PJG ITAG l=26 g=5 f=2 -->
i+1
<!-- PJG /ITAG -->

<!-- PJG ITAG l=26 g=1 f=1 -->

<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
<!-- PJG /QTAG -->
 
<!-- PJG 0012 frnewline -->

