=0.30 * k
<!-- PJG /ITAG -->

<!-- PJG ITAG l=24 g=5 f=2 -->
i 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=24 g=1 f=1 -->
(i > 256) 
<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
<!-- PJG /QTAG -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG ITAG l=21 g=1 f=1 -->
