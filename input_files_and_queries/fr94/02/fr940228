4. El Paso Natural Gas Co. 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=71 g=1 f=1 -->
[Docket No. CP94&hyph;226&hyph;000] 
<!-- PJG 0012 frnewline -->

