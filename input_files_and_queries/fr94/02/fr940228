&sect;668.2 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=89 g=1 f=1 -->
General definitions. 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=11 g=1 f=1 -->
