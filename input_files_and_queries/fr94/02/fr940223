2
<!-- PJG /ITAG -->

<!-- PJG ITAG l=01 g=1 f=1 -->
 
<!-- PJG 007 0104 intable -->
3,770,000 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG 007 0146 intable -->
11993 U.S. Dollars. 
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG 007 0146 intable -->

<!-- PJG /ITAG -->

