<!-- PJG 007 0150 intable -->
1&blank;
<!-- PJG 007 0150 intable -->
1&blank;
<!-- PJG 007 0152 intable -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=01 g=1 f=1 -->
(1) 
<!-- PJG 007 0104 intable -->
$11,000,000 
<!-- PJG 0012 frnewline -->
(2) 
<!-- PJG 007 0104 intable -->
2,000,000 
<!-- PJG 0012 frnewline -->
(3) Because of a setoff under 
<!-- PJG 0012 frnewline -->
&sect;1.482&hyph;1T(e)(5)
<!-- PJG 007 0104 intable -->
9,000,000)
<!-- PJG 007 0162 intable -->
n,s 
