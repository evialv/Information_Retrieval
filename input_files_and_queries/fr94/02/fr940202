<!-- PJG ITAG l=01 g=1 f=1 -->
(1) 
<!-- PJG 007 0104 intable -->
$9,000,000 
<!-- PJG 0012 frnewline -->
(2) Not a 200 percent or 400 percent adjustment 
<!-- PJG 007 0104 intable -->
2,000,000 
<!-- PJG 0012 frnewline -->
(3) 
<!-- PJG 007 0104 intable -->
9,000,000
<!-- PJG 007 0162 intable -->
n,s 
<!-- PJG 0012 frnewline -->

