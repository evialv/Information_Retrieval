46 CFR Part 25
<!-- PJG 0012 frnewline -->
Emergency Position Indicating Radio Beacons and Visual Distress Signals for Uninspected Vessels; Proposed Rule
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG /STAG -->

<!-- PJG /FTAG -->

<!-- PJG FTAG 4701 -->

<!-- PJG STAG 4701 -->

<!-- PJG ITAG l=90 g=1 f=3 -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=90 g=1 f=4 -->
Federal Register
<!-- PJG /ITAG -->

<!-- PJG ITAG l=90 g=1 f=1 -->
&blank;/&blank;Vol. 59, No. 33&blank;/&blank;Thursday, February 17, 1994&blank;/&blank;Proposed Rules
<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=52 g=1 f=1 -->

<USDEPT>DEPARTMENT OF TRANSPORTATION</USDEPT>

<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=18 g=1 f=1 -->

<USBUREAU>Coast Guard</USBUREAU>
<!-- PJG /ITAG -->

<!-- PJG ITAG l=52 g=1 f=1 -->
46 CFR Part 25
<!-- PJG 0012 frnewline -->

